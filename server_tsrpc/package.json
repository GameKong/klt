{
  "dependencies": {
    "mongodb": "^5.1.0",
    "tsrpc": "^3.4.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.1",
    "copyfiles": "^2.4.1",
    "tsrpc-cli": "^2.4.5"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "tsrpc-cli build && npm run copy",
    "doc": "tsrpc-cli doc",
    "proto": "tsrpc-cli proto",
    "sync": "tsrpc-cli sync",
    "api": "tsrpc-cli api",
    "copy": "copyfiles -u 2 ./src/config/*.json ./dist/config/",
    "start:all": "pm2 start ecosystem.config.js --env develop && pm2 monit",
    "shutdown:all": "pm2 delete all",
    "dev:robot": "tsrpc-cli dev --entry src/serverRobotEntry.ts",
    "dev:gate": "tsrpc-cli dev --entry src/serverGateEntry.ts",
    "dev:match": "tsrpc-cli dev --entry src/serverMatch.ts",
    "dev:room": "tsrpc-cli dev --entry src/serverRoomEntry.ts",
    "dev:room2": "cross-env PORT=3002 tsrpc-cli dev --entry src/serverRoomEntry.ts"
  }
}
