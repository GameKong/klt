{
  "openapi": "3.0.0",
  "info": {
    "title": "TSRPC Open API",
    "version": "1.0.0"
  },
  "paths": {
    "/GameArea": {
      "post": {
        "operationId": "GameArea",
        "requestBody": {
          "description": "Req<GameArea>",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PtlGameArea_ReqGameArea"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "description": "ApiReturn<ResGameArea>",
                  "properties": {
                    "isSucc": {
                      "type": "boolean",
                      "enum": [
                        true
                      ],
                      "default": true
                    },
                    "res": {
                      "$ref": "#/components/schemas/PtlGameArea_ResGameArea"
                    }
                  }
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "$ref": "#/components/responses/error"
          }
        }
      }
    },
    "/Login": {
      "post": {
        "operationId": "Login",
        "requestBody": {
          "description": "Req<Login>",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PtlLogin_ReqLogin"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "description": "ApiReturn<ResLogin>",
                  "properties": {
                    "isSucc": {
                      "type": "boolean",
                      "enum": [
                        true
                      ],
                      "default": true
                    },
                    "res": {
                      "$ref": "#/components/schemas/PtlLogin_ResLogin"
                    }
                  }
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "$ref": "#/components/responses/error"
          }
        }
      }
    },
    "/Register": {
      "post": {
        "operationId": "Register",
        "requestBody": {
          "description": "Req<Register>",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PtlRegister_ReqRegister"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "description": "ApiReturn<ResRegister>",
                  "properties": {
                    "isSucc": {
                      "type": "boolean",
                      "enum": [
                        true
                      ],
                      "default": true
                    },
                    "res": {
                      "$ref": "#/components/schemas/PtlRegister_ResRegister"
                    }
                  }
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "$ref": "#/components/responses/error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PtlGameArea_ReqGameArea": {
        "type": "object",
        "properties": {}
      },
      "PtlGameArea_ResGameArea": {
        "type": "object",
        "properties": {
          "area": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "description": "游戏分区名"
                },
                "server": {
                  "type": "string",
                  "description": "服务器地址"
                }
              },
              "required": [
                "name",
                "server"
              ]
            }
          }
        },
        "required": [
          "area"
        ]
      },
      "PtlLogin_ReqLogin": {
        "type": "object",
        "properties": {
          "server": {
            "type": "string",
            "description": "匹配服务器地址"
          },
          "username": {
            "type": "string",
            "description": "用户名"
          }
        },
        "required": [
          "server",
          "username"
        ],
        "description": "登录请求信息"
      },
      "PtlLogin_ResLogin": {
        "allOf": [
          {
            "$ref": "#/components/schemas/___base_BaseResponse"
          },
          {
            "type": "object",
            "properties": {
              "key": {
                "type": "number",
                "description": "玩家唯一标识"
              }
            },
            "required": [
              "key"
            ]
          }
        ],
        "description": "开始匹配响应信息"
      },
      "___base_BaseResponse": {
        "type": "object",
        "properties": {
          "__ssoToken": {
            "type": "string",
            "description": "通行证"
          }
        },
        "description": "HTTP 基础返回数据"
      },
      "PtlRegister_ReqRegister": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "description": "用户名"
          }
        },
        "required": [
          "username"
        ],
        "description": "用户注册请求"
      },
      "PtlRegister_ResRegister": {
        "type": "object",
        "properties": {
          "key": {
            "type": "number"
          }
        },
        "required": [
          "key"
        ],
        "description": "用户注册应响应"
      },
      "?bson_ObjectID": {
        "type": "string"
      },
      "?bson_ObjectId": {
        "type": "string"
      },
      "?mongodb_ObjectID": {
        "type": "string"
      },
      "?mongodb_ObjectId": {
        "type": "string"
      }
    },
    "responses": {
      "error": {
        "description": "Error",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "title": "API 错误",
              "description": "业务错误（ApiError）返回 HTTP 状态码 200，其它错误返回 HTTP 状态码 500",
              "properties": {
                "isSucc": {
                  "type": "boolean",
                  "enum": [
                    false
                  ],
                  "default": false
                },
                "err": {
                  "type": "object",
                  "description": "TsrpcError",
                  "properties": {
                    "message": {
                      "type": "string"
                    },
                    "type": {
                      "type": "string",
                      "enum": [
                        "ApiError",
                        "NetworkError",
                        "ServerError",
                        "ClientError"
                      ]
                    },
                    "code": {
                      "oneOf": [
                        {
                          "type": "string"
                        },
                        {
                          "type": "integer"
                        }
                      ],
                      "nullable": true
                    }
                  },
                  "required": [
                    "message",
                    "type"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}